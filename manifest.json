{
  "manifest_version": 2,

  "name": "Renren Today Photos",
  "description": "This extension shows your photos of Today N years ago",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["*://www.renren.com/*"],
      "js": ["popup.js"]
    }
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}